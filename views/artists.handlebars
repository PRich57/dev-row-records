<!-- BODY -->


<!-- ALL ARTISTS SECTION -->
<div class="container-fluid mx-0 z-class" style = "border: .2rem solid red">
    <div class = "row  mt-5 mb-3">
      <h1 class = "text-center">ARTISTS SECTION</h1>
    </div>
    
    
    <div class = "row z-class" style = "border: .2rem solid blue" id = "artist-div-row">
      {{#each artists as |artist|}}
     <div class = "col-xl-3 col-lg-6 col-12 d-flex align-items-center justify-content-center p-3" style = "border: .2rem solid green">
        <div class="card cardEvent" style="width: 18rem;"  id = "{{id}}">
          <img src="{{filename}}" class="card-img-top img-fluid" alt="..." style = "height: 15rem" id = "artistIndex">
          <div class="card-body d-flex justify-content-center">
            <h5 class="card-title">{{artist_name}}</h5>
          </div>
        </div>
      </div>
      {{/each}}
    
