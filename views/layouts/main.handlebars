    <!-- LOGIN MODAL -->
    <div class="modal fade" id="login-modal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h1 class="modal-title fs-5" id="staticBackdropLabel">Login</h1>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
              <form>
                <div class="mb-3">
                  <label for="message-text" class="col-form-label">Username:</label>
                  <input type="text" class="form-control" id="loginUsernameInput">
                  <label for="message-text" class="col-form-label">Password:</label>
                  <input type="text" class="form-control" id="loginPasswordInput">
                </div>
              </form>
            </div>
            <div class="modal-footer" id = "modal-login-submit-div">
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
              <button type="button" class="btn btn-danger" id = "modal-login-submit" data-bs-dismiss="modal">Login</button>
            </div>
          </div>
        </div>
      </div>
    
      <!-- SIGN UP MODAL -->
    <div class="modal fade" id="signup-modal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h1 class="modal-title fs-5" id="staticBackdropLabel">Sign Up Here</h1>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
              <form>
                <div class="mb-3">
                    <label for="message-text" class="col-form-label">Username:</label>
                    <input type="text" class="form-control" id="signup-username-input" placeholder = "jdoe23">
                    <label for="message-text" class="col-form-label">Password:</label>
                    <input type="text" class="form-control" id="signup-password-input" placeholder="passw0rd_17">
                </div>
              </form>
            </div>
            <div class="modal-footer" id = "modal-signup-submit-div">
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
              <button type="button" class="btn btn-danger" id = "modal-signup-submit" data-bs-dismiss="modal">Sign Up</button>
            </div>
          </div>
        </div>
      </div>
